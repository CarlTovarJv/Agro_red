generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"  
  url      = "file:./dev.db"
}

model users {
  user_id       Int      @id @default(autoincrement())
  first_name    String
  last_name     String
  dui           String   @unique
  address       String
  date_of_birth DateTime
  gender        String
  email         String   @unique
  password      String
  accept_terms  Boolean
  accept_privacy Boolean
}
